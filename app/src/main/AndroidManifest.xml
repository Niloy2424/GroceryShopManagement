<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.niloychowdhury.groceryshopmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ProductsActivity" />
        <activity android:name=".Activity.CategaryActivity" />
        <activity android:name=".Activity.SubCategoryActivity" />
        <activity android:name=".Activity.ShowItemActivity" />
        <activity android:name=".Activity.PointOfSaleActivity" />
        <activity android:name=".Activity.ReportActivity" />
        <activity android:name=".Activity.StockActivity"></activity>
    </application>

</manifest>